    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ahmadpiee/monggo-store/issues"
    },
    "homepage": "https://github.com/ahmadpiee/monggo-store#readme",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "colors": "^1.4.0",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-async-handler": "^1.1.4",
        "mongoose": "^5.13.3"
    },
    "devDependencies": {
        "concurrency": "^0.1.4",
        "concurrently": "^6.2.0",
        "nodemon": "^2.0.12"
    }
}
